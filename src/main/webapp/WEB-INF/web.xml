<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <!--suppress ServletWithoutMappingInspection -->
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.franksang.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <!--suppress WebProperties -->
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <!--suppress ServletWithoutMappingInspection -->
        <servlet-name>GetHeadServlet</servlet-name>
        <servlet-class>com.franksang.servlet.GetHeadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <!--suppress WebProperties -->
        <servlet-name>GetHeadServlet</servlet-name>
        <url-pattern>/GetHeadServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <!--suppress ServletWithoutMappingInspection -->
        <servlet-name>SetHeadServlet</servlet-name>
        <servlet-class>com.franksang.servlet.SetHeadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <!--suppress WebProperties -->
        <servlet-name>SetHeadServlet</servlet-name>
        <url-pattern>/SetHeadServlet</url-pattern>
    </servlet-mapping>
</web-app>